{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "max-line-length": [true, 120],
        "semicolon": [true, "always"],
        "quotemark": [true, "single"],
        "no-console": [false],
        "object-literal-sort-keys": [true, "match-declaration-order"],
        "ordered-imports": false
    },
    "rulesDirectory": []
}
